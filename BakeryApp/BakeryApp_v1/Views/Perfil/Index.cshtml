@{
    Layout = "_LayoutAdministrador";
}



<div>
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Perfiles</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li><a href="@Url.Action("CrearPerfil", "Perfil")" class="btn btn-sm btn-primary">Crear nuevo perfil</a></li>
                    </ol>
                </div>
            </div>
        </div>
    </section>




    @Html.AntiForgeryToken()
    <section class="content" id="tabla">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">Historial de Perfiles.</h3>
                    </div>
                    <div class="card-body table-responsive">
                        <table class="table table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>#</th>
                                    <th>Nombre</th>
                                    <th>Primer Apellido</th>
                                    <th>Segundo Apellido</th>
                                    <th>Correo</th>
                                    <th>Teléfono</th>
                                    <th>Rol</th>
                                    <th>Editar</th>
                                    <th>Eliminar</th>
                                </tr>
                            </thead>
                            <tbody id="agregar">
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>



@section Scripts {
    <script type="text/javascript" src="~/js/perfil/index.js" asp-append-version="true">

    </script>
}